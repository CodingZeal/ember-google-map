<div class="map-canvas"></div>
<div class="markers" style="display: none;">
  <ul>
    {{#each marker in markers itemController=markerController itemView=markerViewClass}}
      <li>{{title}} @ {{lat}},{{lng}}</li>
      {{#if view.hasInfoWindow}}
        {{view 'google-map/info-window'}}
      {{/if}}
    {{/each}}
  </ul>
  <ul>
    {{each infoWindows itemController=infoWindowController itemView=infoWindowViewClass}}
  </ul>
</div>
